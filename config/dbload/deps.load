LOAD DATABASE
FROM postgresql://postgres:postgres@192.168.1.188/gitlab-usage
INTO postgresql://postgres:postgres@localhost/gitlab-usage

WITH create tables,
batch rows = 5000,
concurrency = 2,
preserve index names

CAST type timestamp to timestamptz

ALTER TABLE NAMES MATCHING ~/^dependencies$/ RENAME TO dependencies_temp

BEFORE LOAD DO
$$ DROP TABLE IF EXISTS dependencies_temp CASCADE; $$;